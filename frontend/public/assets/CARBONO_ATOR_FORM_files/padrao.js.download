function  FocusPrimeiroItem( indexForm )
{
	try
	{
		if ( document.forms.length > 0 )
		{
			for ( i = 0; i <= document.forms[indexForm].length; i++ )
			{
				if ( document.forms[indexForm][i].type != "hidden" && document.forms[indexForm][i].disabled == false )
				{
					document.forms[indexForm][i].focus();
					i = document.forms[indexForm].length + 1;
				}
			}
		}
	}
	catch( e  )
	{
		
	}
}

