/* 
*@description 	Descrição da Função JS
*@				
*@author			  André Ribeiro arer76@hotmail.com
*@data          16/08/2019
*@param         definição de parâmetros de houver
*/
function  janela(URL,L,A,barra,tools){
    // declara as vari veis
    // quando uma vari vel n o for uma caracter stica de um objeto.
    var scrol
    var htal
    var vcal
    // fim da declara  o
    // verifica se o valor do tamanho da janela   igual a zero
    // verifica largura
    if (L=='0'){
    L= screen.width;
    }
    // verifica altura
    if (A=='0'){
    A= screen.height;
    }
    // recebe a altura da janela
    var height = A;
    // recebe a largura da janela
    var width = L;
    // recebe o balor e diz se tem ou n o barra de rolagem
    var scrol = barra;
      if (scrol == ''){
      scrol = 'no';
      }
    // recebe o valor e adiciona ou n o a barra de ferramentas
    var tool = tools;
      if (tool == ''){
      tool = 'no';
    }
    // htal <-- recebe a metade da largura da janela menos a metade do tamanho da janela
    htal = ( screen.width / 2 ) - ( L / 2 );
    // htal <-- recebe a metade da altura da janela menos a metade do tamanho da janela
    vcal = ( screen.height / 2 ) - ( A / 2 );
    window.open (URL,'Pop_UP','width='+width+',height='+height+',left='+htal+',top='+vcal+',toolbar='+tool+',location=no,status=no,menu=no,scrollbars='+scrol+',resizable=no,titlebar=no');	
}
