function  envia_form(strID,strURL,strIdDestino){
  var dados = $('#'+strID).serialize();
  //console.log(dados)
  $.ajax({
      //enctype: 'multipart/form-data',
      url: strURL,
      type: 'POST', // Tipo de requisição, podendo alterar para GET, POST, PUT , DELETE e outros metodos http
      data: dados,
      success: function (resposta){
        //Bloco com a resposta do servidor caso a requisição ocorra normalmente
        if (strIdDestino!=''){
          $('#'+strIdDestino).html(resposta);
        }else{
          console.log(resposta);
        }
      },
      error: function (resposta){
        //Entrará aqui caso não de certo a requisição 
        //console.log('Erro no processamento!');
        $('#'+strIdDestino).html(resposta);
        //console.log(resposta);
      }
  })
}

/* 
* 
*@description 	Função para adicionar uma opção a um elemento SELECT.
*@author			  André Ribeiro arer76@hotmail.com
*@data          18/11/2024
* @param {string} selectId - O ID do elemento SELECT.
* @param {string} texto - O texto a ser exibido na opção.
* @param {string} valor - O valor da opção.
*/
function atualizarSelect(selectId, dados) {
    // Obtém o elemento SELECT pelo ID
    const select = document.getElementById(selectId);

    // Limpa o conteúdo do SELECT
    select.innerHTML = '';
    //console.log(dados);
    // Adiciona uma opção padrão
    const opcaoPadrao = document.createElement("option");
    if (dados == '0') {
      opcaoPadrao.text = "Nao localizado!";
      opcaoPadrao.value = -1;
    }else{
      opcaoPadrao.text = "Selecione uma opção";
      opcaoPadrao.value = 0;
    }
    select.add(opcaoPadrao);

    // Popula o SELECT com novos dados
    dados.forEach(item => {
        const novaOpcao = document.createElement("option");
        novaOpcao.text = item.texto;
        novaOpcao.value = item.valor;
        select.add(novaOpcao);
    });

    //console.log("SELECT atualizado com sucesso!");
}

