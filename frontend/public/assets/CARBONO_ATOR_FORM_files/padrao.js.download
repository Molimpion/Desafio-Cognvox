function  redimensionaarquivos(strNomeArquivo,intLargura,intAltura){
      // Envia o formulário via AJAX usando jQuery
      // Cria um novo objeto FormData para enviar o arquivo e os parâmetros adicionais
      var formData = new FormData();

      // Adiciona outros parâmetros ao FormData
      formData.append('filename', strNomeArquivo); // Nome do arquivo (se for diferente do input file)
      formData.append('largura', intLargura); // Largura desejada
      formData.append('altura', intAltura); // Altura desejada

      // Envia o formulário via AJAX usando jQuery
      $.ajax({
          url: '/md_ARQUIVOS/redimensiona/', // Endpoint no servidor que processa o upload
          type: 'POST',
          data: formData, // Enviar o FormData
          success: function (response) {
              // Exibe a resposta do servidor no console ou em um div
              console.log('Sucesso no redimensionamento!', response);
          },
          error: function (xhr, status, error) {
              // Exibe uma mensagem de erro e imprime detalhes no console
              console.error('Erro no envio do arquivo:', xhr.responseText, status, error);
          }
      });
  };

