/* 
*@description 	Descrição da Função JS
*@				
*@author			  André Ribeiro arer76@hotmail.com
*@data          16/08/2019
*@param         definição de parâmetros de houver
*/
function  enviaarquivos(localExibicao,strNomeArquivo,strNomeArquivoHidden){
   // Verifica se um arquivo foi selecionado
   var fileInput = $('#'+strNomeArquivo)[0]; // Obtém o input de upload de arquivo
   var file = fileInput.files[0]; // Pega o primeiro arquivo selecionado

   if (!file) {
       msgmodal('AVISO!', 'Por favor, selecione um arquivo.', ''); // Mensagem de aviso
       return;
   } else {
       $('#'+localExibicao).html('<img class="caixa" width="200" src="/images/carregando1.gif">'); // Imagem de carregando

       // Cria um objeto FormData para armazenar o arquivo e enviar via AJAX
       var formData = new FormData();
       formData.append('arquivo', file); // Adiciona o arquivo ao FormData
       
       // Envia o formulário via AJAX usando jQuery
       $.ajax({
           url: '/md_ARQUIVOS/UPLOAD/', // Endpoint no servidor que processa o upload
           type: 'POST',
           data: formData,
           processData: false,
           contentType: false,
           enctype: 'multipart/form-data',
           success: function (response) {
               // Exibe a resposta do servidor no div #result
               $('#'+localExibicao).html('<img class="caixa col-md-6" src="/md_ARQUIVOS/UPLOAD/deposito/'+response+'">'); // Imagem de carregando
               if(strNomeArquivoHidden !==''){
                 $('#'+strNomeArquivoHidden).val(response);
               }
               msgmodal('AVISO!', 'Arquivo Enviado.', ''); // Mensagem de aviso
           },
           error: function (xhr, status, error) {
               // Exibe uma mensagem de erro e imprime detalhes no console
               console.error('Erro no envio do arquivo:', xhr.responseText, status, error);
               msgmodal('AVISO!', 'Erro no envio do arquivo.', ''); // Mensagem de aviso
           }
       });

   }
};
