function RecuperaVideoAWS(strUrl,strDestino){
  // Configura as credenciais da AWS
  AWS.config.update({
      accessKeyId: '123123123123',
      secretAccessKey: '123123123',
      region: 'us-east-2'
  });
  // // Cria uma instância do objeto S3 da AWS
  var s3 = new AWS.S3();
  // Função para obter a URL do arquivo no S3
  var params = {
  Bucket: 'cognvox-video2/Compositions',
  Key: strUrl,
  Expires: 4000
  };
  s3.getSignedUrl('getObject', params, function (err, url) {
  if (err) {
      console.log('Erro ao obter URL assinada', err);
  } else {
      // Aqui você pode usar o URL assinado para configurar o vídeo
      document.getElementById(strDestino).src = url;
      // console.log('URL assinada:', url);
  }
  });  
  
}  

/* 
*@description 	Retorna um vídeo na AWS
*@				
*@author			  André Ribeiro arer76@hotmail.com
*@data          16/08/2019
*@param         definição de parâmetros de houver
*/
function RecuperaArquivoAWS(strUrl,strDestino,strTipoDestino){
  // Configura as credenciais da AWS
  AWS.config.update({
      accessKeyId: '123',
      secretAccessKey: '213',
      region: 'us-east-2'
  });
  // // Cria uma instância do objeto S3 da AWS
  var s3 = new AWS.S3();
  // Função para obter a URL do arquivo no S3
  var params = {
  Bucket: 'cognvox-video2/Compositions',
  Key: strUrl,
  Expires: 4000
  };
  s3.getSignedUrl('getObject', params, function (err, url) {
  if (err) {
      console.log('Erro ao obter URL assinada', err);
  } else {
      // Aqui você pode usar o URL assinado para configurar o vídeo
      switch (strTipoDestino) {
        case 'src':
          
          document.getElementById(strDestino).src = url;
          break;
      
        case 'value':
          
          document.getElementById(strDestino).value = url;
          break;
      
        case 'href':
          
          document.getElementById(strDestino).href = url;
          break;
      
        case 'onclick':
          
          document.getElementById(strDestino).onclick = url;
          break;
      
        default:
          break;
      }
      // console.log('URL assinada:', url);
  }
  });  
  
}  