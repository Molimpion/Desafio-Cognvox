*@description 	Descrição da Função JS
*@				
*@author			  André Ribeiro arer76@hotmail.com
*@data          16/08/2019
*@param         definição de parâmetros de houver
*/
function  valida_campos(intSessao,intAluno,intFrame){

  ////console.log(intSessao,intAluno,intFrame)
  document.getElementById("checkcompreencao").style.display = "none";
  document.getElementById("checkindicadoresgestuais").style.display = "none";
  document.getElementById("checkindicadoresdiscursivos").style.display = "none";
  document.getElementById("textosframe").style.display = "none";
  document.getElementById("textosframecompreencao").style.display = "none";
  document.getElementById("textosframecritica").style.display = "none";
  document.getElementById("textosframedirecionamento").style.display = "none";
  document.getElementById("textosframegestosobservados").style.display = "none";
  document.getElementById("textosframeturnofala").style.display = "none";
  
  //Direcionamento
  if( $("#tf44").is(":checked") == true ){
    abretextosframedirecionamento(intSessao,intAluno,intFrame);
    document.getElementById("textosframedirecionamento").style.display = "block";
  }
  //Preenchimento de Lacuna
  if( $("#tf45").is(":checked") == true ){
    abreTextoFrame(intSessao,intAluno,intFrame);
    document.getElementById("textosframe").style.display = "block";
  }
  //Crítica
  if( $("#tf46").is(":checked") == true ){
    document.getElementById("textosframecritica").style.display = "block";
    abretextosframecritica(intSessao,intAluno,intFrame);
  }
  //Compreensão
  if( $("#tf47").is(":checked") == true ){
    abreCheckFrameCompreencao(intSessao,intAluno,intFrame);
    abretextosframecompreencao(intSessao,intAluno,intFrame);
    document.getElementById("textosframecompreencao").style.display = "block";
    document.getElementById("checkcompreencao").style.display = "block";
  }
  //turno Fala
  if( $("#di93").is(":checked") == true ){
    abretextosframeturnofala(intSessao,intAluno,intFrame);
    document.getElementById("textosframeturnofala").style.display = "block";
  }
  //Gestos Observados
  if( $("#di95").is(":checked") == true ){
    abretextosframegestosobservados(intSessao,intAluno,intFrame);
    document.getElementById("textosframegestosobservados").style.display = "block";
  }
  //indicadores Discursivos
  if( $("#in53").is(":checked") == true ){
    abretextosframeindicadoresdiscursivos(intSessao,intAluno,intFrame)
    document.getElementById("checkindicadoresdiscursivos").style.display = "block";
  }       
  //indicadores Gestuais
  if( $("#in54").is(":checked") == true ){
    abretextosframeindicadoresgestuais(intSessao,intAluno,intFrame)
    document.getElementById("checkindicadoresgestuais").style.display = "block";
  }   

  abreResumoFrame(intSessao,intAluno,intFrame);    
  
}
